		<!-- Finder methods -->

		<finder name="TEN_TH" return-type="Collection">
			<finder-column name="ten" />
			<finder-column name="trangThai" />
		</finder>
	</entity>	
	
	<exceptions>
		<exception>HetHanKhuyeMai</exception>
	</exceptions>
</service-builder>